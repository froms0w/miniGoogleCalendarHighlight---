{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0.0",
    "description": "This is my first Chrome extension.",
    "permissions": [
      "activeTab",
      "storage",
      {
        "matches": ["https://calendar.google.com/*"]
      }
    ],
    "web_accessible_resources": [
      "style.css"
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    }
  }
  